<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title %></title>
    <%= stylesheet_link_tag "application" %>
    <%= javascript_include_tag "application" %>
    <%= favicon_tag 'images/am.ico' %>

    <!-- Facebook Open Graph data -->
    <meta property="og:url"                content="https://demtzu.github.io/portfolio/" />
    <meta property="og:type"               content="portfolio" />
    <meta property="og:title"              content="<%= current_page.data.title %>" />
    <meta property="og:description"        content="Full Love Web developper living between Brussels and Paris" />
    <meta property="og:image"              content="https://demtzu.github.io/portfolio/images/ahmed-portfolio.jpg" />
  </head>
  <body>
    <%= yield %>
    <%= google_analytics_tag %>
  </body>
</html>
